{
    "name": "algoritma/php-coding-standard",
    "description": "Algoritma coding standard",
    "type": "composer-plugin",
    "extra": {
        "class": "Algoritma\\CodingStandards\\Installer\\Plugin"
    },
    "license": "MIT",
    "keywords": [
        "algoritma",
        "algoritma.it",
        "coding standard"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "composer": "*",
        "composer-plugin-api": "*",
        "bitexpert/phpstan-magento": "^0.29.0",
        "codeception/codeception": "^5.0",
        "composer/composer": "^1.3.2 || ^2.0",
        "friendsofphp/php-cs-fixer": "^3.9",
        "friendsoftwig/twigcs": "^6.2",
        "icanhazstring/composer-unused": "^0.8.7",
        "nunomaduro/larastan": "^2.5",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-symfony": "^1.2",
        "rector/rector": "^0.15.23",
        "symfony/console": "^4.0 || ^5.0 || ^6.0",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Algoritma\\CodingStandards\\": "src/"
        }
    }
}
